<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_kabul_rapor" model="ir.sequence">
            <field name="name">Kabul Raporu Numarası</field>
            <field name="code">kabul.rapor.sequence</field>
            <field name="prefix">KBL/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_teslim_rapor" model="ir.sequence">
            <field name="name">Teslim Raporu Numarası</field>
            <field name="code">teslim.rapor.sequence</field>
            <field name="prefix">TSL/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
