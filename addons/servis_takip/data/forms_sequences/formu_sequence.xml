<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_kabul_formu" model="ir.sequence">
            <field name="name">Kabul Formu Numarası</field>
            <field name="code">kabul.formu.sequence</field>
            <field name="prefix">KBL/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_teslim_formu" model="ir.sequence">
            <field name="name">Teslim Formu Numarası</field>
            <field name="code">teslim.formu.sequence</field>
            <field name="prefix">TSL/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
